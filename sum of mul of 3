import java.util.*;
class prefixMulsum
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int q=sc.nextInt();
int n=1000;
int[] prefix=new int[n+1];
prefix[0]=3;
for(int i=1;i<n+1;i++)
{
prefix[i]=prefix[i-1]*i; 
}
for(int i=0;i<q;i++)
{
int sum=sc.nextInt();
System.out.println(prefix[sum]);
}
}
}
