import java.util.*;
import java.lang>*;
class BiggestOf4
{
public static void main(String args[])
{
int a=5,b=10,c=3,d=6;
if((a>b)&(a>c)&(a>d))
System.out.println("a is greater");
if((b>a)&(b>c)&(b>d))
System.out.println("b is greater");
if((c>a)&(c>b)&(c>d))
System.out.println("c is greater");
else
System.out.println(" d is greater");
}
}
